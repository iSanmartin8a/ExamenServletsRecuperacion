<!DOCTYPE web-app PUBLIC
 "-//Sun Microsystems, Inc.//DTD Web Application 2.3//EN"
 "http://java.sun.com/dtd/web-app_2_3.dtd" >

<web-app>

  <display-name>Archetype Created Web Application</display-name>
  
  <servlet>
  	<servlet-name>GameServlet</servlet-name>
  	<display-name>GameServlet</display-name>
  	<description></description>
  	<servlet-class>servlets.GameServlet</servlet-class>
  </servlet>
  
  <servlet>
  	<servlet-name>ConsoleServlet</servlet-name>
  	<display-name>ConsoleServlet</display-name>
  	<description></description>
  	<servlet-class>servlets.ConsoleServlet</servlet-class>
  </servlet>
  
  <servlet-mapping>
  	<servlet-name>GameServlet</servlet-name>
  	<url-pattern>/GameServlet</url-pattern>
  </servlet-mapping>
  
  <servlet-mapping>
  	<servlet-name>ConsoleServlet</servlet-name>
  	<url-pattern>/ConsoleServlet</url-pattern>
  </servlet-mapping>
  
</web-app>
